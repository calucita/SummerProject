<script type="text/javascript">
	var myID_cookie = Utility.MembershipId();
	if (Utility.IsAuthenticated() && viewModels.loggedInUserModelIsLoaded() == false) {
		PageUrls.myProfilePage = "/en/User/Profile?id=0MID0".replace('0MID0', Utility.MembershipId());
		bungieNetPlatform.userService.GetCurrentUser(
		function (result) {
				$(document).find("head").append("<link href='" + Utility.ContentVersioned("/img/UserThemes/" + result.user.profileThemeName + "/skin.css")+"' rel='stylesheet' type='text/css'>");
		    viewModels.loggedInUserModel(ko.mapping.fromJS(result));
		    viewModels.loggedInUserModelIsLoaded(true);
		    

			if (viewModels.loggedInUserModel().adultMode()) {
				// Remove any existing image hiding elements.
				$(".post_urlLinkOrImage").addClass("deferred");
				$(".btn_hideImage").addClass("hide");
				// Remove any existing age gate prompts.
				$(".ageGatePrompt").addClass("hide");
				$(".ageGateContents").removeClass("hide");
			}
		},
		function (error) {
			if (error.errorCode == 5) {
				window.location = "/Errors/bnetOfflineError.html";
			}
		});
	}
	

	$(document).ready(function () {
		//localstorage deleted for different user
		if (Modernizr.localstorage) {

			if (typeof localStorage["myID_cookie"] !== "undefined" && localStorage.getItem("myID_cookie") != myID_cookie) {
			    localStorage.removeItem("myID_cookie")
			}

		}
	});

	Utility.alertSuccess = function (message, successMessageType) {
		if (viewModels.loggedInUserModel()) {
			if ((viewModels.loggedInUserModel().user.successMessageFlags() & successMessageType) != successMessageType) {
				var checkbox = "<p class='successMessageContainer container_checkbox'><input type='checkbox' class='successMessageCheckbox' data-successMessageType='" + successMessageType + "' /> ";
				checkbox += Localizer.Messages.dontshowmessagecheckbox;
				checkbox += "</p>";
				var finalMessage = message + checkbox;
				Utility.alert(finalMessage, true);
				$("#alert").find(".successMessageContainer").bind("click.lightbox", function (e) {
					// Don't close the box if they click in here
					e.stopImmediatePropagation();
				});
				$("#alert").find(".successMessageCheckbox").bind("change", function (e) {
					var doNotShow = $(this).is(":checked");
					if (doNotShow) {
						// add the flag.
						viewModels.loggedInUserModel().user.successMessageFlags(viewModels.loggedInUserModel().user.successMessageFlags() | successMessageType);
					} else {
						// Remove the flag (AND the inverse of the flag)
						viewModels.loggedInUserModel().user.successMessageFlags(viewModels.loggedInUserModel().user.successMessageFlags() & ~successMessageType);
					}

					bungieNetPlatform.userService.EditSuccessMessageFlags(
						viewModels.loggedInUserModel().user.successMessageFlags(),
						function (result) {
							// Now that we've changed the flag, close the lightbox.
							customAlert.closeLightbox();
						},
						function (error) {
							// If we couldn't change the flag, it isn't the end of the world.  We'll have logged whatever the problem was, let's not bother the user with it.
						}
					);
				});
			} else {
				customAlert.closeLightbox();
			}
		} else {
			// If no logged in user or for some reason it hasn't loaded yet, just show the message as-is.
			Utility.alert(message, true);
		}
	};

</script>


	<script src="/sharedbundle/shared?v=o2UyiO4n_oovg1F6gM6lZcut_JmtGP3lOEo0z808Jq41"></script>



<script type="text/javascript">
	// Set the user minimum age if we have that info.
	if (Modernizr.localstorage)
	{
		if (localStorage["minimumAge_cookie"])
		{
			viewModels.userMinimumAge = ko.observable(localStorage["minimumAge_cookie"]);

			$(document)
				.ready(function() {
					// Any server-side age gates should be taken down.
					if (viewModels.userMinimumAge())
					{
						$(".ageGateContents")
							.each(function() {
								var contents = $(this);

								var minimumAge = parseInt(contents.attr("data-minimumAge"), 10);

								if (viewModels.userMinimumAge() >= minimumAge)
								{
									contents.removeClass("hide");
									contents.parent().find(".ageGatePrompt").addClass("hide");
									contents.parent().find(".ageGateForbidden").addClass("hide");
								}
							});
					}
				});
		}
		else
		{
			viewModels.userMinimumAge = ko.observable(0);
		}
	}
	else
	{
		viewModels.userMinimumAge = ko.observable(0);
	}

	viewModels.userMinimumAge.subscribe(function(newValue) {
		if (Modernizr.localstorage)
		{
			localStorage["minimumAge_cookie"] = newValue;
		}
	});
</script>

