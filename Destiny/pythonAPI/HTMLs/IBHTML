		bungieNetPlatform.platformSettings.initialize({
			platformUrl: "/Platform",
			baseUrl: "/",
			currentLocale: 'en',
			locInherit: 'true' == 'true',
			isMobileDevice: 'false' == 'true',
			isTouchDevice: Modernizr.touch,
			renderContentEditLinks: 'false' == 'true',
			internalLinkRegex:
				/https?\:\/\/(((www)|(betanext)|(alpha)|(nike)|(bnetdev)|(bnetint)|(static-internal)|(local-static)|(static01))\.bungie(store)?\.(net|com))/gi,
			contentVersion: "50735.16.05.27.0243-10"
		});

		bungieNetPlatform.defaultData = {};

		templateEngine.version = "cv=3983621215&av=1083700426";

		//cancels all outstanding ajax calls via the bungienetPlatform
		$(window)
			.on("unload", function() {
				for (var i = 0; i < currentRequests.length; i++)
				{
					currentRequests[i].abort();
				}
			});
	}
	catch (err)
	{
	}

	window.onload = function()
	{
		$('body').addClass('loaded');
		//not supported message for browsers with no generated content
		if (!Modernizr.generatedcontent)
		{
			$(".bg_top").append("<span>You are using an unsupported browser, your experience may be less than optimal</span>");
		}

		if (typeof loadAddThis !== 'undefined')
		{
			loadAddThis();
		}
	};

</script>



